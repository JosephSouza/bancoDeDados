	public static void main(String[] args) {
		
		Scanner leitor = new Scanner(System.in);
		UsuarioDao usuarioDao = new UsuarioDao();
		System.out.println("Digite CÃ³digo para excluir");
		usuarioDao.excluir(leitor.nextInt());
		leitor.close();
	}

}
